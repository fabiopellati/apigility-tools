{
    "name": "fabiopellati/apigility-tools",
    "description": "General tools for Apigility",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Fabio Pellati",
            "email": "fabio.pellati@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://fabiopellati@github.com/fabiopellati/message-exchange-event-manager.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laminas/laminas-view": "^2.9",
        "laminas/laminas-db": "^2.8",
        "laminas/laminas-hydrator": "^2.2",
        "laminas/laminas-serializer": "^2.8",
        "laminas/laminas-filter": "^2.7",
        "laminas/laminas-paginator": "^2.7",
        "laminas-api-tools/api-tools": "^1.3",
        "laminas/laminas-uri": "^2.5",
        "laminas/laminas-cache": "^2.7",
        "laminas/laminas-stdlib": "^3.1",
        "fabiopellati/message-exchange-event-manager": "^3",
        "php": "^7.3",
        "laminas/laminas-dependency-plugin": "^2.1"
    },
    "require-dev": {
        "laminas/laminas-test": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "ApigilityTools\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "extra": {
        "laminas": {
            "component": [
                "Laminas\\ApiTools\\Rest",
                "Laminas\\ApiTools\\ApiProblem",
                "MessageExchangeEventManager",
                "ApigilityTools"
            ]
        }
    }
}